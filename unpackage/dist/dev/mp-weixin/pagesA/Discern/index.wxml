<view class="pageBox" style="{{'width:'+(width+'px')+';'+('height:'+(height+'px')+';')}}"><view class="status_bar"></view><view class="topBox" style="{{'width:'+(width+'px')+';'}}"><image class="topBoxBg" style="{{'width:'+(width+'px')+';'}}" src="https://xin.yiyunoto.com/uploads/static/bao/beijing.png" mode="scaleToFill"></image><view data-event-opts="{{[['tap',[['back',['$event']]]]]}}" class="backPage" bindtap="__e"><image class="backImg" src="../../static/public/goBack.png"></image></view><view class="editBox"><image class="editImg" src="../../static/discern/edit.png" mode></image></view><view class="headBox"><image class="headBorder" src="{{imgurl+img}}" mode></image><cover-image class="headImg" src="{{userInfo.image}}"></cover-image></view><text class="headText" style="{{'width:'+(width+'px')+';'}}">{{userInfo.name}}</text></view><view class="contentBox" style="{{'width:'+(width+'px')+';'+('height:'+(height-167+'px')+';')}}"><text class="titleTxt">列表详情</text><scroll-view class="itemList" style="{{'width:'+(width+'px')+';'+('height:'+(height-225+'px')+';')}}" scroll-y="true" lower-threshold="{{100}}" show-scrollbar="{{false}}" data-event-opts="{{[['scrolltolower',[['getLogList',[true]]]]]}}" bindscrolltolower="__e"><block wx:for="{{listData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item" style="{{'width:'+(width*0.95+'px')+';'}}"><image class="itemHeadImg" src="{{wuyeurl+item.path}}" mode></image><view class="itemDataInfo" style="{{'width:'+(width*0.95*0.7+'px')+';'}}"><view class="itemTitleBox" style="{{'width:'+(width*0.95*0.7+'px')+';'}}"><text class="itemTitleTxt">{{item.name}}</text><view class="itemTitleCoef"><text class="itemTitleCoefTxt">{{"识别度:"+item.score+"%"}}</text></view></view><text class="itemBottomTxt">{{"刷脸时间："+item.create_time}}</text></view></view></block></scroll-view></view></view>