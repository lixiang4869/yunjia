.btn {
  position: absolute;
  z-index: 999;
  top: 540rpx;
  margin: auto 17% auto;
  width: 66%;
  height: 110rpx;
  background: white;
  -webkit-box-shadow: 0rpx 9rpx 10rpx 0rpx rgba(120, 142, 200, 0.2);
          box-shadow: 0rpx 9rpx 10rpx 0rpx rgba(120, 142, 200, 0.2);
  -webkit-border-radius: 55rpx;
          border-radius: 55rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: horizontal;
  -webkit-box-direction: normal;
  -webkit-flex-direction: row;
          flex-direction: row;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
}
.btn .leftImg {
  margin-left: 20rpx;
  margin-top: 10rpx;
}
.btn .leftImg image {
  width: 91rpx;
  height: 91rpx;
}
.btn .open {
  width: 145rpx;
  font-size: 36rpx;
  font-family: Microsoft YaHei;
  font-weight: bold;
  color: white;
  background: -webkit-gradient(linear, right top, left top, from(rgba(100, 159, 251, 0.68)), to(#2c6df5));
  background: -webkit-linear-gradient(right, rgba(100, 159, 251, 0.68) 0%, #2c6df5 100%);
  background: linear-gradient(-90deg, rgba(100, 159, 251, 0.68) 0%, #2c6df5 100%);
  -webkit-background-clip: text;
  -webkit-text-fill-color: transparent;
}
.btn .rightImg {
  margin-right: 60rpx;
}
.btn .rightImg image {
  width: 32rpx;
  height: 40rpx;
}
.sunui-slider {
  position: relative;
  overflow: hidden;
  width: 602rpx;
  height: 110rpx;
  line-height: 110rpx;
  margin: 0 auto;
  -webkit-border-radius: 55rpx;
          border-radius: 55rpx;
  -webkit-box-shadow: 0rpx 0rpx 4rpx rgba(0, 0, 0, 0.1);
          box-shadow: 0rpx 0rpx 4rpx rgba(0, 0, 0, 0.1);
  font-size: 32rpx;
  text-align: center;
  background-color: #F0AD4E;
}
.sunui-slider-bg {
  position: absolute;
  overflow: hidden;
  top: 0;
  width: 101%;
  height: 110rpx;
  line-height: 110rpx;
  background-color: #7BBB55;
  -webkit-border-radius: 55rpx;
          border-radius: 55rpx;
  color: #fff;
}
.sunui-slider-box {
  position: absolute;
  right: 0;
  top: 0;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  /* margin-top: 10rpx; */
  height: 90rpx;
  width: 90px;
  /* background-color: #EBEBEB; */
  background-size: 100% 100%;
}

