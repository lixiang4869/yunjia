.video {
  width: 200rpx;
  height: 200rpx;
  background: #ffffff;
}
.video_close {
  text-align: center;
  line-height: 30rpx;
  height: 35rpx;
  width: 35rpx;
  background: #ef5350;
  color: #ffffff;
  position: absolute;
  top: 0rpx;
  right: 0rpx;
  font-size: 35rpx;
  -webkit-border-radius: 8rpx;
          border-radius: 8rpx;
  z-index: 100000;
}
.close-view {
  text-align: center;
  line-height: 30rpx;
  height: 35rpx;
  width: 35rpx;
  background: #ef5350;
  color: #ffffff;
  position: absolute;
  top: 1rpx;
