<scroll-view style="{{'height:'+(height)+';'}}" scroll-y="{{true}}" show-scrollbar="{{false}}" refresher-enabled="{{true}}" data-event-opts="{{[['scrolltoupper',[['onPullDown',['$event']]]]]}}" bindscrolltoupper="__e"><view class="pullDown" style="{{'width:'+(width)+';'+('top:'+(topY)+';')+('position:'+(isDown?'relative':'absolute')+';')}}"><block wx:if="{{downImgShow}}"><image style="width:50rpx;height:50rpx;" src="../../static/video/lodin.gif"></image></block><text style="color:#fff;font-size:28rpx;">{{pullDownTxt}}</text></view><view class="livePageBox" style="{{'width:'+(width)+';'}}"><view class="bannerBox"><uni-swiper-dot style="width:710rpx;" vue-id="35f0106a-1" info="{{info}}" current="{{current}}" field="content" mode="{{mode}}" bind:__l="__l" vue-slots="{{['default']}}"><swiper data-event-opts="{{[['change',[['change',['$event']]]]]}}" class="swiper-box" style="width:710rpx;height:285rpx;" bindchange="__e"><block wx:for="{{info}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item style="width:710rpx;height:285rpx;"><view class="swiper-item" style="width:710rpx;height:285rpx;"><image style="width:710rpx;height:285rpx;" src="{{item.img}}"></image></view></swiper-item></block></swiper></uni-swiper-dot></view><view class="recommend"><view class="leftTitle"><image class="leftTitleImg" src="../../static/live/recom.png"></image><text class="leftTitleTxt">主播力荐</text></view><scroll-view class="shopList" scroll-x="true" show-scrollbar="{{false}}"><view class="shopView" style="{{'width:'+(190*goods.length+'upx')+';'}}"><block wx:for="{{goods}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['goShopCart',['$0','$1'],[[['goods','',index,'shopId']],[['goods','',index,'good_id']]]]]]]}}" class="shopItem" bindtap="__e"><image class="shopItemImg" src="{{item.goodImg}}"></image><text class="shopItemTitle">{{item.txt}}</text></view></block></view></scroll-view></view><view class="liveList"><block wx:for="{{lists}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="liveItem"><view class="liveItemTop"><image class="liveHead" src="{{item.headimg||'../../static/video/defaultHead.png'}}"></image><view class="liveInfo"><text class="liveTitle">{{item.title}}</text><view class="subheadBox"><text style="font-size:25rpx;color:#999;">{{item.tags}}</text><text style="font-size:25rpx;color:#999;">{{item.addtime}}</text></view></view><view class="topLiveStateBox"><view class="topLiveStateDot"></view><block wx:if="{{item.living}}"><text class="topLiveStateTxt">直播中</text></block><block wx:if="{{!item.living}}"><text class="topLiveStateTxt">未直播</text></block></view></view><text class="synopsis"></text><view class="liveImgBox"><view data-event-opts="{{[['tap',[['go_video',['$0','$1'],[[['lists','',index,'id']],[['lists','',index,'streamname']]]]]]]}}" class="bigLiveImg" bindtap="__e"><image class="bigImg" src="{{item.img}}"></image><image class="playLive" src="https://xin.yiyunoto.com/uploads/static/live/live.png"></image><view class="stateInfoBox"><block wx:if="{{item.living==1}}"><text class="stateInfoLeft">•直播中</text></block><block wx:if="{{item.living==0}}"><text class="stateInfoLeft">•未直播</text></block></view><view class="likeNumBox"><image style="width:45rpx;height:45rpx;" src="https://xin.yiyunoto.com/uploads/static/live/like.png"></image><text style="color:#fff;font-size:20rpx;">{{item.keep_num}}</text></view></view><view class="miniLiveImg"><image class="miniImg" src="{{item.good.length>=1?url+item.good[0].img:''}}"></image><image class="miniImg" src="{{item.good.length>=2?url+item.good[1].img:''}}"></image></view></view></view></block><view class="liveBottom"></view></view></view></scroll-view>