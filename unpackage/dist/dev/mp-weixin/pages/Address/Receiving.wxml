<view class="content "><block wx:for="{{addressList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{addressList.length>0}}"><view class="list"><view data-event-opts="{{[['tap',[['checkAddress',['$0'],[[['addressList','',index]]]]]]]}}" class="wrapper" bindtap="__e"><view class="address-box"><view><text class="name">{{item.name}}</text><text class="mobile">{{item.phone}}</text><block wx:if="{{item.defaults==2}}"><text class="tag">默认</text></block></view><view class="righticon"><image class="bianji" src="https://xin.yiyunoto.com/uploads/static/address/bianji.png" mode="widthFix" data-event-opts="{{[['tap',[['bianji',['$0'],[[['addressList','',index]]]]]]]}}" catchtap="__e"></image><image class="del" src="https://xin.yiyunoto.com/uploads/static/address/del.png" mode="widthFix" data-event-opts="{{[['tap',[['del',[index,'$0'],[[['addressList','',index]]]]]]]}}" catchtap="__e"></image></view></view><view class="u-box"><text class="address">{{item.addrs+" "+item.addrmore}}</text></view></view></view></block></block><view class="save"><view data-event-opts="{{[['tap',[['addaddress',['$event']]]]]}}" class="btn" bindtap="__e">添加地址</view></view></view>